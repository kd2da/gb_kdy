package operator;

public class Main03 {
	public static void main(String[] args) {
		int plus_name = 1;
		System.out.println(plus_name);
		plus_name = plus_name + 1; // 2
		System.out.println(plus_name);
		plus_name += 1; // 3
		System.out.println(plus_name);
		plus_name++; // 4
		System.out.println(plus_name);
		++plus_name; // 5
		System.out.println(plus_name);
		
		int minus_name = 5;
		System.out.println(minus_name);
		plus_name = plus_name - 1; // 2
		System.out.println(minus_name);
		plus_name -= 1; // 3
		System.out.println(minus_name);
		plus_name--; // 4
		System.out.println(minus_name);
		--plus_name; // 5
		System.out.println(minus_name);
	}
}
